         if (data.authenticated) {
           $("#log_out").show();
           $("#log_in").hide();
           $("#add_recipe").show();
           $("#sign_up").hide();
           $("#profile").show();
           $(".upvotebutton").show();
           $(".voted").hide();
         } else {
           $("#log_out").hide();
           $("#log_in").show();
           $("#add_recipe").hide();
           $("#sign_up").show();
           $("#profile").hide;
           $(".upvotebutton").hide();
           $(".voted").hide();
         }
      }
    });
};
</script>


